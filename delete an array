#include<stdio.h>
main()
{
  int a[10],n,i;
   printf("enter the size of array :: \n");
  scanf("%d",&n);
printf("enter the elements :: \n");
  for(i=1;i<=n;i++)
  {
    scanf("%d",&a[i]);
  }
  int k;
  printf("enter the postion to delete :: \n");
  scanf("%d",&k);
  for(i=k;i<=n;i++)
  {
    a[i]=a[i+1];
  }
  for(i=1;i<=n-1;i++)
  {
    printf("%d  ",a[i]);
  }
  
}
